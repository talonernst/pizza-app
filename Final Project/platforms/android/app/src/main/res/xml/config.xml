<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.TAndTPizzeria" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>T &amp; T Pizzeria</name>
    <description>
        Pizza App made by two friends for a final project in their Mobile Applications class
        make orders, leave reviews and edit them, and be added to an email list in this awesome app!
    </description>
    <author email="ternst6858@conestogac.on.ca" href="http://conestogac.on.ca">
        Talon Ernst, Taylor McVittie
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="pizzaIcon.png" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
